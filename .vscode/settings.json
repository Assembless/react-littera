{
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": false,
    "**/out": true,
    "**/coverage": false,
    ".awcache": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/out": false,
    "**/dist": false,
    "**/coverage": false,
    "tslint-rules/**/*.js": true,
    ".awcache": true,
    ".eslintcache": true
  },
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  }
}