{
  "collectCoverageFrom": ["src/**/*.js"],
  "coveragePathIgnorePatterns": ["/node_modules/", "<rootDir>/src/example", "<rootDir>/src/index"],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "transform": {
    ".*": "<rootDir>/node_modules/babel-jest"
  },
  "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.js"
}
