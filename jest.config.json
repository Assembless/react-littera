{
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/example",
      "<rootDir>/src/index"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.js"
  }